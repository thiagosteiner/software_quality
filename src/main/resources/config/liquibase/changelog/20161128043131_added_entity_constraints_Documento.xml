<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity Documento.
    -->
    <changeSet id="20161128043131-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="aluno_id"
                                 baseTableName="documento"
                                 constraintName="fk_documento_aluno_id"
                                 referencedColumnNames="id"
                                 referencedTableName="aluno"/>

        <addForeignKeyConstraint baseColumnNames="documentocomite_id"
                                 baseTableName="documento"
                                 constraintName="fk_documento_documentocomite_id"
                                 referencedColumnNames="id"
                                 referencedTableName="comite"/>

        <addForeignKeyConstraint baseColumnNames="documentoorientador_id"
                                 baseTableName="documento"
                                 constraintName="fk_documento_documentoorientador_id"
                                 referencedColumnNames="id"
                                 referencedTableName="professor"/>

    </changeSet>
</databaseChangeLog>
