<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity Convite.
    -->
    <changeSet id="20161128043132-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="aluno_id"
                                 baseTableName="convite"
                                 constraintName="fk_convite_aluno_id"
                                 referencedColumnNames="id"
                                 referencedTableName="aluno"/>

        <addForeignKeyConstraint baseColumnNames="professor_id"
                                 baseTableName="convite"
                                 constraintName="fk_convite_professor_id"
                                 referencedColumnNames="id"
                                 referencedTableName="professor"/>

        <addForeignKeyConstraint baseColumnNames="conviteprofessor_id"
                                 baseTableName="convite"
                                 constraintName="fk_convite_conviteprofessor_id"
                                 referencedColumnNames="id"
                                 referencedTableName="professor"/>

        <addForeignKeyConstraint baseColumnNames="convite_comite_id"
                                 baseTableName="convite"
                                 constraintName="fk_convite_convite_comite_id"
                                 referencedColumnNames="id"
                                 referencedTableName="comite"/>

        <addForeignKeyConstraint baseColumnNames="convite_documento_id"
                                 baseTableName="convite"
                                 constraintName="fk_convite_convite_documento_id"
                                 referencedColumnNames="id"
                                 referencedTableName="documento"/>

    </changeSet>
</databaseChangeLog>
