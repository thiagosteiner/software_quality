<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity Proposta.
    -->
    <changeSet id="20161031124408-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="aluno_id"
                                 baseTableName="proposta"
                                 constraintName="fk_proposta_aluno_id"
                                 referencedColumnNames="id"
                                 referencedTableName="aluno"/>

        <addForeignKeyConstraint baseColumnNames="comite_id"
                                 baseTableName="proposta"
                                 constraintName="fk_proposta_comite_id"
                                 referencedColumnNames="id"
                                 referencedTableName="comite"/>

    </changeSet>
</databaseChangeLog>
