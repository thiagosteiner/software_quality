<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity Aluno.
    -->
    <changeSet id="20161202222223-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableName="aluno"
                                 constraintName="fk_aluno_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>

        <addForeignKeyConstraint baseColumnNames="alunos_id"
                                 baseTableName="aluno_documento"
                                 constraintName="fk_aluno_documento_alunos_id"
                                 referencedColumnNames="id"
                                 referencedTableName="aluno"/>
        <addForeignKeyConstraint baseColumnNames="documentos_id"
                                 baseTableName="aluno_documento"
                                 constraintName="fk_aluno_documento_documentos_id"
                                 referencedColumnNames="id"
                                 referencedTableName="documento"/>
        
    </changeSet>
</databaseChangeLog>
