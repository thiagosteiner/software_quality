<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity Aluno.
    -->
    <changeSet id="20161031172227-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="monografia_id"
                                 baseTableName="aluno"
                                 constraintName="fk_aluno_monografia_id"
                                 referencedColumnNames="id"
                                 referencedTableName="monografia"/>

        <addForeignKeyConstraint baseColumnNames="alunos_id"
                                 baseTableName="aluno_publicacao"
                                 constraintName="fk_aluno_publicacao_alunos_id"
                                 referencedColumnNames="id"
                                 referencedTableName="aluno"/>
        <addForeignKeyConstraint baseColumnNames="publicacaos_id"
                                 baseTableName="aluno_publicacao"
                                 constraintName="fk_aluno_publicacao_publicacaos_id"
                                 referencedColumnNames="id"
                                 referencedTableName="publicacao"/>
        
        <addForeignKeyConstraint baseColumnNames="professor_id"
                                 baseTableName="aluno"
                                 constraintName="fk_aluno_professor_id"
                                 referencedColumnNames="id"
                                 referencedTableName="professor"/>

    </changeSet>
</databaseChangeLog>
