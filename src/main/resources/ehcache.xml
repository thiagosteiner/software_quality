<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd"
         name="CM1"
         updateCheck="false"
         maxBytesLocalHeap="16M">

    <!--
      This is a default configuration, it is re-configured by the CacheConfiguration Spring Bean, using the
      properties from the resources/config/*.yml files.
    -->

    <diskStore path="java.io.tmpdir"/>

    <defaultCache
            eternal="false"
            overflowToDisk="false"
            />

    <cache name="br.ufrj.cos.qsoftware.domain.User"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="br.ufrj.cos.qsoftware.domain.Authority"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="br.ufrj.cos.qsoftware.domain.User.authorities"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="br.ufrj.cos.qsoftware.domain.PersistentToken"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="br.ufrj.cos.qsoftware.domain.User.persistentTokens"
           timeToLiveSeconds="3600">
    </cache>
    
    <cache name="br.ufrj.cos.qsoftware.domain.Aluno"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="br.ufrj.cos.qsoftware.domain.Aluno.propostas"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="br.ufrj.cos.qsoftware.domain.Aluno.publicacaos"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="br.ufrj.cos.qsoftware.domain.Professor"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="br.ufrj.cos.qsoftware.domain.Professor.alunos"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="br.ufrj.cos.qsoftware.domain.Professor.orientadorpublicacaos"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="br.ufrj.cos.qsoftware.domain.Departamento"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="br.ufrj.cos.qsoftware.domain.Departamento.professors"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="br.ufrj.cos.qsoftware.domain.Monografia"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="br.ufrj.cos.qsoftware.domain.Proposta"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="br.ufrj.cos.qsoftware.domain.Comite"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="br.ufrj.cos.qsoftware.domain.Comite.professors"
        timeToLiveSeconds="3600">
